use_frameworks!
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

xcodeproj 'OCBaseProject.xcodeproj'

target 'OCBaseProject' do

# UIKit相关  ReactiveCocoa 高版本用法不一样。暂时用这个版本
pod 'ReactiveCocoa', '~> 2.5'
pod 'MBProgressHUD'
pod 'MJRefresh'
pod 'MJExtension'
pod 'Masonry'
# YYKit相关
pod 'YYKit', '~> 1.0.9',:inhibit_warnings => true


# 网络
pod 'AFNetworking'
# OC 版本的leancloud
pod 'AVOSCloud'



end
